<h1>Log in</h1>

    <%= form_for(:session) do |f| %>

      <%= f.label :'Nickname' %>
      <%= f.text_field :nickname %>
	  </br>
      <%= f.label :'password' %>
      <%= f.password_field :password %>
	  </br>
      <%= f.submit "Log in"%>
    <% end %>

    <p>New user? <%= link_to "Sign up now!", register_path %></p>
